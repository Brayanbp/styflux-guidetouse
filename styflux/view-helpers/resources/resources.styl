			background-position actionIconHighlightDis
			&:hover
				background-position actionIconHighlightDis
	&.givedown
		background-position actionIconGivedown
		width 18px
		&:hover
			background-position actionIconGivedownHover
	&.publishads
		background-position actionIconPublicAds
		width 18px
		&:hover
			background-position actionIconPublicAdsHover
		&.dis
			background-position actionIconPublicAdsDisabled
			cursor default
	&.republish
		background-position actionIconRepublish
		&:hover
			background-position actionIconRepublishHover
	&.delete
		background-position actionIconDelete
		&:hover
			background-position actionIconDeleteHover
	&.note
		background-position actionIconNote
		&:hover
			background-position actionIconNoteHover
	&.message
		background-position actionIconMessage
		&:hover
			background-position actionIconMessageHover
	&.share
		background-position actionIconShare
		&:hover
			background-position actionIconShareHover
	&.calendar
		background-position actionIconCalendar
		width 19px
		&:hover
			background-position actionIconCalendarHover
	&.properties
		background-position actionIconProperty
		&:hover
			background-position actionIconPropertyHover
	&.play
		background-position actionIconPlay
		&:hover
			background-position actionIconPlayHover
	&.pending
		background-position actionIconPending
		&:hover
			background-position actionIconPendingHover
	&.addprinted
		background-position actionIconAddPrinted
		&:hover
			background-position actionIconAddPrintedHover
	&.cancelprinted
		background-position actionIconCancelPrinted
		&:hover
			background-position actionIconCancelPrintedHover
	&.seeinfo
