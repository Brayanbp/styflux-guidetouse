/**
@module resources.css
@submodule Iconos de action para el admin
*/
.action-icons
	*display inline
	*zoom 1
	background-image url(routImg+"icon-publicacion.png")
	background-repeat no-repeat
	display inline-block
	font-size 0px
	height 21px
	line-height 0px
	vertical-align middle
	width 20px
	& + .action-icons
		margin-left 10px
	&.edit
		background-position actionIconEdit
		height 21px
		width 18px
		&:hover
			background-position actionIconEditHover
	&.highlight
		background-position actionIconHighlight
		width 18px
		&:hover
			background-position actionIconHighlightHover
		&.act
			background-position actionIconNoHighlight
			&:hover
				background-position actionIconNoHighlightHover
			&.bloq
				background-position actionIconNoHighlightDis
				&:hover
