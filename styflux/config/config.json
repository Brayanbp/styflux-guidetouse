{
	"compile":{
		"ext":".styl",
		"cmd":"stylus -c",
		"cmdr":"-o",
		"route":"../css"
	},
	"layouts":{
		"active":true,
		"path":"./layouts",
		"compile":{
			"layout.styl":{
				"helpers":{
					"fonts":"fonts.styl",
					"content":"content.styl",
					"reset":"reset.styl",
					"grids":"grids.styl",
					"buttons":"buttons.styl",
					"forms":"forms.styl",
					"table":"table.styl",
					"plugins":"plugins.styl",
					"resources":["resources.styl"],
					"hacks":"[@ult]hacks.styl"
				}
			}
		}
	},
	"views":{
		"path":"./views",
		"pathCompile":"./views"
	},
	"vars":{
		"styfluxFonts":"[@url]fonts/",
		"routImg":"[@url]../img/",
		"family1":"'lucidaRegular',Arial,sans-serif",
		"family2":"'abrazRegular',Arial,sans-serif",
		"family3":"Arial,sans-serif",
		"Styflux":"17px 50%"
	},
	"ignore":{
	}
}